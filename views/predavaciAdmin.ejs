

<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css' integrity='sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z' crossorigin='anonymous' />
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1>Svi predavaci su prikazani u tabeli ispod</h1>

<a href="/admin/naslovna">Povratak na početnu</a>
<br><br><br>

<div class="mx-auto">
    <table id = "pp" class="table table-striped table-bordered">
        <thead>
        <tr>
            <th> Ime  </th>
            <th> Prezime </th>

            <th>Izbrisi predavaca</th>
            <th>Banuj
            </th>
            <th>Banuj</th>
        </tr>
        </thead>
        <tbody>
        <% predavaci.forEach(dokument => { %>
            <tr>
                <td> <%= dokument.name %> </td>
                <td> <%= dokument.surname %> </td>


                <td><button  class="btn btn-sm btn-danger"><a href="/admin/predavac/izbrisi?id=<%= dokument._id %>">Briši</a></button></td>
                <td><button  class="btn btn-sm btn-danger"><a href="/admin/banuj30?id=<%= dokument._id %>">30 dana</a></button></td>
                <td><button  class="btn btn-sm btn-danger"><a href="/admin/banuj15?id=<%= dokument._id %>">15 dana</a></button></td>





            </tr>
        <% }); %>





        </tbody>
    </table>
</div>
<h3>Banovani predavaci</h3>
<div class="mx-auto">
    <table id = "pb" class="table table-striped table-bordered">
        <thead>
        <tr>
            <th> Ime  </th>
            <th> Prezime </th>


            <th></th>

        </tr>
        </thead>
        <tbody>
        <% banovani.forEach(dokument => { %>
            <% predavaci.forEach(predavac => {%>

            <tr>
                <% if (dokument.id_predavaca == predavac._id){%>
                <% console.log("NASAO SAM JEDNOG")%>
                <td> <%= predavac.name %> </td>
                <td> <%= predavac.surname %> </td>



                <td><button  class="btn btn-sm btn-danger"><a href="/admin/odbanuj?id=<%= predavac._id %>">Odbanuj</a></button></td>




            </tr>
                <% }; %>
            <% }); %>
        <% }); %>





        </tbody>
    </table>
</div>



</body>
</html>




